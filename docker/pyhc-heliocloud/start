#!/bin/bash -l

# ==== ONLY EDIT WITHIN THIS BLOCK =====

export HELIOCLOUD_ENV="helio-notebook-base"
if ! [[ -z "${HELIOCLOUD_SCRATCH_PREFIX}" ]] && ! [[ -z "${JUPYTERHUB_USER}" ]]; then
    export HELIOCLOUD_SCRATCH="${HELIOCLOUD_SCRATCH_PREFIX}/${JUPYTERHUB_USER}/"
fi

# ==== ONLY EDIT WITHIN THIS BLOCK =====

exec "$@"
